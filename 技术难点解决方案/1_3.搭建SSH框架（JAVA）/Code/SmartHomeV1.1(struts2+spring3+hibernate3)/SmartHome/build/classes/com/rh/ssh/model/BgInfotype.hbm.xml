<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.rh.ssh.model">
	<class name="BgInfotype" table="bg_infotype">
		<id name="infoTypeId" type="java.lang.Integer">
			<column name="info_type_id" />
			<generator class="native"></generator>
		</id>
		<property name="infoTypeName" type="java.lang.String">
			<column name="info_type_name"></column>
		</property>
	</class>
</hibernate-mapping>